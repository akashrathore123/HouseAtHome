<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web_1="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>House At Home</display-name>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>SessionLessFilter</filter-name>
    <filter-class>com.landportal.filter.SessionLessFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>SessionLessFilter</filter-name>
    <url-pattern>/profile.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>SessionLessFilter</filter-name>
    <url-pattern>/commercialSell.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>SessionLessFilter</filter-name>
    <url-pattern>/residentialSell.jsp</url-pattern>
  </filter-mapping>

  <filter>
    <filter-name>SessionFullFilter</filter-name>
    <filter-class>com.landportal.filter.SessionFullFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>SessionFullFilter</filter-name>
    <url-pattern>/login.jsp</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>CookieCheck</filter-name>
    <filter-class>com.landportal.filter.CookieCheck</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CookieCheck</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>sell</servlet-name>
    <servlet-class>com.landportal.sell.Sell</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sell</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>signout</servlet-name>
    <servlet-class>com.landportal.login.SignOut</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signout</servlet-name>
    <url-pattern>/foo/*</url-pattern>
  </servlet-mapping>
</web-app>